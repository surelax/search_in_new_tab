{
  "manifest_version": 3,
  "name": "Search In New Tab",
  "description": "This extension provides a context menu to search selected text with Google in a new tab.",
  "version": "0.0.1",
  "background": {
    "service_worker": "search_in_new_tab.js"
  },
  "icons": {
    "16": "icons/icon16x16.png",
    "48": "icons/icon48x48.png",
    "128": "icons/icon128x128.png"
  },
  "permissions": ["contextMenus"]
}